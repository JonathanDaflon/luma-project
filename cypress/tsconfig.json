{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es6",
    "module": "nodenext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "lib": ["ESNext", "dom"],
    "types": ["cypress", "node"],
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["**/*.ts"]
}
