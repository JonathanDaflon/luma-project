{
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": false,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "lib": ["es6", "dom"],
    "types": ["cypress", "node"],
    "skipLibCheck": true
  },
  "include": ["**/*.ts"]
}
