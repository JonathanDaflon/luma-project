{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es6",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "lib": ["ESNext", "dom"],
    "types": ["cypress", "node", "cypress-mochawesome-reporter"],
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["**/*.ts"]
}
